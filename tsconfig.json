{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
   "paths": {
      "@/*": ["./*"],
      "@/src/*": ["src/*"]
    }
  },
  "include": [
    "app",
    "src",
    "**/*.ts",
    "**/*.tsx"
, "babel.config.js"  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
